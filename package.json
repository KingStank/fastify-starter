{
  "name": "fastify-starter",
  "version": "1.0.0",
  "description": "A starter for Fastify with Twig, Alpine, and Tailwind.",
  "main": "app.js",
  "scripts": {
    "test": "tap tests/**/*.test.js",
    "start": "fastify start -l info app.js",
    "dev": "npm-run-all -p dev:*",
    "dev:fastify": "fastify start -w -l info -P app.js",
    "dev:proxy": "sleep 3 && browser-sync start -c bs.config.js",
    "dev:webpack": "webpack --watch",
    "gen:key": "secure-session-gen-key > secret-key"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "alpinejs": "^2.8.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.3.1",
    "fastify-cli": "^2.7.0",
    "fastify-plugin": "^3.0.0",
    "fastify-secure-session": "^2.3.1",
    "fastify-static": "^4.0.0",
    "mongoose": "^5.11.17",
    "point-of-view": "^4.13.0",
    "twig": "^1.15.4",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "autoprefixer": "^10.2.4",
    "browser-sync": "^2.26.14",
    "css-loader": "^5.0.2",
    "mini-css-extract-plugin": "^1.3.8",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.6",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^5.0.0",
    "tailwindcss": "^2.0.3",
    "tailwindcss-debug-screens": "^2.0.0",
    "tap": "^14.11.0",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0"
  }
}
